<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" href="../CSS/style.css">
</head>
<body>
    
    <div class="row">
        <div class="left_section"> </div>

        <div class="middle_section">
            <table>
                <tr>
                <th>Fields</th>
                <th>Values</th>
                </tr>
                
                <tr>
                <td>Country</td>
                <td>{{country}}</td>
                </tr>

                <tr>
                <td>City</td>
                <td>{{city}}</td>
                </tr>
            
                <tr>
                    <td>Region</td>
                    <td>{{region}}</td>
                </tr>

                <tr>
                    <td>ObservationTime</td>
                    <td>{{observation_time}}</td>
                </tr>

                <tr>
                    <td>Humidity</td>
                    <td>{{humidity}}</td>
                </tr>

            </table>
        
        </div>
    
        <div class="right_section"></div>

    </div>
   
</body>
</html>